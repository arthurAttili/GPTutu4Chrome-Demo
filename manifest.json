{
    "manifest_version": 3,
    "name": "GPTutu-Demo",
    "version": "1.0",
    "description": "Private use extension for GPT-4 queries",
    "action": {
        "default_title": "GPTutu-Demo",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icons/icon_16.png",
        "48": "icons/icon_48.png",
        "128": "icons/icon_128.png"
    },
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ],
    "background.service_worker": {
        "scripts": ["popup.js", "apiGPT.js"]
    }
}
